{
  "name": "demo-ecommerce",
  "description": "AKVA is a Hydrogen starter that presents a real-world example of how Sanity and Structured Content can elevate your custom Shopify storefronts.",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "_dev": "turbo run dev --parallel",
    "dev": "turbo run dev --filter=./apps/*",
    "lint": "turbo run lint",
    "lint:fix": "npm run lint -- -- --fix",
    "format": "turbo run format",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@turbo/gen": "^1.11.3",
    "husky": "^8.0.3",
    "turbo": "^1.11.3",
    "vercel": "^32.7.2"
  },
  "packageManager": "pnpm@8.10.2",
  "dependencies": {
    "@sanity/block-tools": "3.25.0",
    "@sanity/cli": "3.25.0",
    "@sanity/diff": "3.25.0",
    "@sanity/export": "3.25.0",
    "@sanity/import": "3.25.0",
    "@sanity/import-cli": "3.25.0",
    "@sanity/mutator": "3.25.0",
    "@sanity/portable-text-editor": "3.25.0",
    "@sanity/schema": "3.25.0",
    "@sanity/types": "3.25.0",
    "@sanity/util": "3.25.0",
    "@sanity/vision": "3.25.0",
    "create-sanity": "3.25.0",
    "groq": "3.25.0",
    "sanity": "3.25.0",
    "styled-components": "6.1.8",
    "@sanity/preview-kit": "^3.2.6",
    "@sanity/presentation": "0.12.4-pink-lizard",
    "@sanity/overlays": "2.3.7",
    "@sanity/react-loader": "0.10.0-pink-lizard"
  },
  "pnpm": {
    "overrides": {
      "@sanity/block-tools": "$@sanity/block-tools",
      "@sanity/cli": "$@sanity/cli",
      "@sanity/diff": "$@sanity/diff",
      "@sanity/export": "$@sanity/export",
      "@sanity/import": "$@sanity/import",
      "@sanity/import-cli": " $@sanity/import-cli",
      "@sanity/mutator": "$@sanity/mutator",
      "@sanity/portable-text-editor": "$@sanity/portable-text-editor",
      "@sanity/schema": "$@sanity/schema",
      "@sanity/types": "$@sanity/types",
      "@sanity/util": "$@sanity/util",
      "@sanity/vision": "$@sanity/vision",
      "create-sanity": "$create-sanity",
      "groq": "$groq",
      "sanity": "$sanity",
      "styled-components": "$styled-components",
      "@sanity/preview-kit": "$@sanity/preview-kit",
      "@sanity/presentation": "$@sanity/presentation",
      "@sanity/overlays": "$@sanity/overlays",
      "@sanity/react-loader": "$@sanity/react-loader"
    }
  }
}
